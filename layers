# Layer configuration for .bash
dotbash use tmux                # always load this one first

dotbash use cd-aliases
dotbash use git-aliases
dotbash use ls-aliases

dotbash use prompt

dotbash use docker
dotbash use history
dotbash use lambda
dotbash use nfu

dotbash use cd all

